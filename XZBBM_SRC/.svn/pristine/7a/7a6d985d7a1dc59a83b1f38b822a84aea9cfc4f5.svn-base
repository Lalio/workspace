@charset "utf-8";
@import "base/mixins";
.fl{
	float: left;
}
.fr{
	float: right;
}
.oh{
	overflow: hidden;
}
body{
	// background:#f4f4f4;
	min-width:1000px;
}
.xz-hd{
	background:#1bc8f6;
}
// 右侧内容区域
.main , .left-col{
	float: left;
	min-height:500px;

}
.container{
	overflow: hidden;
}
.main{
	width:100%;
	.content{
		margin-left: 210px ;
		background: #fff;
		min-height: 500px;
		border-left:1px solid #e5ebf1;
		padding-bottom: 125px;
	}
	.page{
		width: 100%;
		display: none;
		overflow: hidden;
		.user__control{
			width: 100%;
			height: 30px;
			margin:10px 0;
			li{
				position: relative;
				height: 28px;
				line-height: 28px;
				width: 120px;
				border: 1px solid #3cc4f4;
				border-radius:5px;
				text-indent: 35px;
				cursor: pointer;
				float: left;
				font-size: 14px;
				.user-con-ico{
					position: absolute;
					top: 8px;
					left: 16px;
					width: 12px;
					height: 12px;
				}
			}
			.upload{
				background-color: #0bb5f1;
				color: #fff;
				margin:0 25px;
				.user-con-ico{
					background: url(../img/user/upload-icon.png) 0 0 no-repeat;
				}
				&:hover{
					background-color: #3cc4f4;
				}
			}
			.com-contorl{
				color: #0bb5f1;
				.user-con-ico{
					background: url(../img/user/del-ico.png) 0 0 no-repeat;
				}
				&:hover{
					color: #fff;
					background-color: #3cc4f4;
					.user-con-ico{
						background: url(../img/user/del-hover.png) 0 0 no-repeat;
					}
				}

			}
		}
		.user__table{
			width: 100%;
			border-top:1px solid #e5ebf1;
			tr{
				font-size: 14px;
				height: 50px;
				border-bottom:1px solid #e5ebf1;
				th{
					color: #999;
					text-align: left;
				}
				.all-select{
					cursor: pointer;
					float: right;
					margin-right: 40px;
				}
				.checkbox{
					outline: 0;
					float: right;
					margin-right: 40px;
				}
				.col{
					color: #999;
				}
				td{
					.passage{
						color: #333;
						float: left;
						width: 400px;
						height: 20px;
						@include ellipsis;
					}
				}
			}
		}
	}
	#ziliao-div{
		display: block;
	}
	#setting-div{
		display: none;
		padding: 20px 0 0 30px;
		.setting-page{
			width: 92%;
			.page-title{
				font-size: 24px;
				line-height: 50px;
				color: #0bb5f1;
				border-bottom: 3px solid #0bb5f1;
			}
		}
		.profit{
			.profit-info{
				height: 103px;
				width: 100%;
				margin: 35px 0;
				margin-right: -30px;
				.profit-list{
					height: 103px;
					width: 21%;
					float: left;
					border:1px solid #eeeeee;
					background-color: #f7f7f7;
					margin-right: 25px;
					.profit-subtitle{
						color: #666;
						font-size: 14px;
						margin: 12px 0 0 10px;
					}
					.profit-subbd{
						font-size: 36px;
						color: #f8a010;
						text-align: center;
						margin-top: 12px;
					}
				}
			}
			.profit__line{
				height: 405px;
				width: 100%;
				overflow: hidden;
				background-color: #f7f7f7;
				margin-bottom: 40px;
				h3{
					font-size: 18px;
					color: #f8a010;
					margin: 16px 0 0 30px;
				}
			}
		}
		.info{
			.my-info{
				margin-top: 35px;
			}
			li{
				width: 100%;
				// height: 40px;
				overflow: hidden;
				margin-bottom: 20px;
				.info-wrap{
					overflow: hidden;
					border:1px solid #eee;
					background-color: #f7f7f7;
				}
				.change{
					width: 99.8%;
					border-left: 1px solid #eee;
					border-right: 1px solid #eee;
					border-bottom: 1px solid #eee;
					padding: 15px 0;
					display: none;
					p{
						line-height: 40px;
						font-size: 14px;
						color: #333;
						margin-left: 150px;
						input{
							outline: 0;
							width: 165px;
							padding: 5px 10px;
						}
					}
					.change-submit{
						width: 60px;
						height: 30px;
						line-height: 30px;
						color: #fff;
						text-align: center;
						border-radius: 15px;
						background-color: #0bb5f1;
						cursor: pointer;
						margin-top: 10px;
						margin-left: 280px;
					}
					.change-info{
						// margin-left: 30px;
					}
					.change-name{
						@include inline-block;
						width: 100px;
						line-height: 40px;
						text-align: right;
						margin-right: 30px;
					}
				}
				.info-detail{
					float: left;
					line-height: 40px;
					font-size: 14px;
					color: #333;
					overflow: hidden;
				}
				.info-name{
					width: 25%;
					height: 40px;
					text-indent: 25px;
				}
				.info-bd{
					width: 60%;
					height: 40px;
				}
				.info-control{
					width: 15%;
					height: 40px;
					color: #0bb5f1;
					text-align: right;
					i{
						margin-right: 20px;
						cursor: pointer;
					}
				}
			}
		}
	}
}
// 左边导航
.left-col{
	width:210px;
	margin-left:-100%;
	background:#F6FBFE;
}
.user-box{
	width:110px;
	margin:15px auto 20px;
	text-align: center;
}
.user-headPic{
	width:80px;
	height:80px;
	margin:0 auto;
	.headPic{
		border-radius:50%;
	}
}
.user-info{
	margin-top:10px;
	font-size:14px;
	color:#777;
	p{
		line-height: 24px;
	}
	.name{
		margin-top:2px;
		width: 110px;
		@include ellipsis;
	}
	.num{
		color: #0BB5F2;
	}
}
.action-list{
	width:100%;
	margin:0 auto;
	padding-bottom:18px;
	.item{	
		font-size: 14px;
		color: #777;
		text-align: center;
		height: 45px;
		line-height: 45px;
		cursor: pointer;
		.inner{
			display: block;
			font-size: 14px;
			color: #777;
			text-align: center;
			height: 45px;
			line-height: 45px;
			border-top:1px solid #E6EBEF;
			border-left:3px solid #F6FBFE;
			text-decoration: none;
			outline: 0;
			&:hover{
				background:#AEE5F9;
				border-top:1px solid #AEE5F9;
				border-left:3px solid #08B7EC;
				color:#0BB5F2;
			}
		}
	}
	.active .inner{
		background:#AEE5F9;
		border-top:1px solid #AEE5F9;
		border-left:3px solid #08B7EC;
		color:#0BB5F2;
	}
}

//loading
.loading{
	width: 190px;
	height: 37px;
	margin: 5px auto;
	position: relative;
	p{
		font-size: 28px;
		color: #a7a7a7;
		text-indent: 65px;
	}
	span{
		position: absolute;
		left: 10px;
		top: 6px;
		width: 30px;
		height: 30px;
		background: url(../img/ploading.gif) 0 0 no-repeat;
	}
}



