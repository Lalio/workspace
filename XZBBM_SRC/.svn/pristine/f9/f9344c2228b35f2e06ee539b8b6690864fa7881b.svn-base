//计算跨行的函数
//算区域跨行的
function cmp_area_rowspan($area){
      $i=0;
	  foreach($area as $k =>$v){
	        foreach($v as $kk=>$vv){
			 
			    foreach($vv as $kkk=>$vvv){
				      $i++;
				}
			}
	  }
  
   return $i;
}
//算广告位跨行
function cmp_adPos_rowspan($adPos){
      $j=0;
	  foreach($adPos as $k =>$v){
	        foreach($v as $kk=>$vv){
			   $j++;
			}
	  }	

	return $j;
}

//算数据跨行的
function cmp_data_rowspan($data){

    return count($data);
}

//计算每组PV OR CLICK的汇总数
function cmp_ad_totals($arr){
	 $total=0;
     foreach($arr as $aid =>$date_data){
	      
		   foreach($date_data as $day =>$data){
		           $total+=(int)$data;
		   }
	 }

	 return $total;
}

