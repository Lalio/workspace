<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.fcv.vms.dao">

	<class name="com.fcv.vms.dao.RegMap" table="reg" lazy="false">

		<id name="id" type="java.lang.Integer" column="ID">
			<generator class="identity" />
		</id>

		<property name="vin" type="java.lang.String" column="VIN"
			length="50" />
		
		<property name="has_data_table" type="java.lang.Integer" column="has_data_table"
			length="6" />


	</class>
</hibernate-mapping>
