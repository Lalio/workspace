package com.fcv.vms.model;

public class WorkReg {
	private String vin;
	private int mark;
	private int istrouble ;
	

	public WorkReg(String vin, int mark, int istrouble){
		this.vin = vin;
		this.mark = mark;
		this.istrouble = istrouble;
	}
	
	
	public String getVin() {
		return vin;
	}
	public void setVin(String vin) {
		this.vin = vin;
	}
	public int getMark() {
		return mark;
	}
	public void setMark(int mark) {
		this.mark = mark;
	}
	public int getIstrouble() {
		return istrouble;
	}
	public void setIstrouble(int istrouble) {
		this.istrouble = istrouble;
	}

	
}
