<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.fcv.vms.pojo">

	<class name="Processing" table="processing_ave" lazy="false">

		<id name="id" type="java.lang.Integer" column="id">
			<generator class="identity" />
		</id>

		<property name="vin" type="java.lang.String" column="vin"
			length="20" />

		<property name="endTime" type="java.util.Date" column="endTime" />
			
		<property name="timeType" type="java.lang.Integer" column="timeType"></property>
	</class>
</hibernate-mapping>
